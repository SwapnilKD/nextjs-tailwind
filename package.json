{
  "name": "next-starter-tailwind",
  "version": "2.0.0",
  "description": "Next.js starter styled with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:dependencies": "npm audit fix",
    "fix:import-sort": "import-sort --write '**/*.{js,jsx}'",
    "analyze:lint": "eslint --ext .js --ext .jsx .",
    "fix:lint": "npm run analyze:lint -- --fix",
    "analyze:prettier": "prettier --list-different \"**/*.{css,md,mdx,js,jsx,json}\"",
    "fix:prettier": "prettier --write '**/*.{css,md,mdx,js,jsx,json}'",
    "analyze:tailwind-css-class-order": "rustywind --dry-run .",
    "fix:tailwind-css-class-order": "rustywind --write .",
    "fix": "npm run fix:dependencies && npm run fix:lint && npm run fix:tailwind-css-class-order && npm run fix:prettier && npm run fix:import-sort",
    "dev": "next",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylorbryant/next-starter-tailwind.git"
  },
  "author": "Taylor Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorbryant/next-starter-tailwind/issues"
  },
  "homepage": "https://github.com/taylorbryant/next-starter-tailwind#readme",
  "dependencies": {
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.9.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "prettier": "2.1.2",
    "rustywind": "0.6.7",
    "tailwindcss": "1.8.10"
  }
}
