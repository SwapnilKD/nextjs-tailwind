{
  "name": "next-starter-tailwind",
  "version": "1.0.0",
  "description": "Next.js starter styled with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "analyze:eslint": "eslint --ext .jsx --ext .js . --fix",
    "analyze:import-sort": "import-sort '**/*.js'",
    "analyze:prettier": "prettier --write '**/*.{js,css,md,mdx}'",
    "analyze:rustywind": "rustywind --dry-run .",
    "analyze": "npm run analyze:eslint && npm run analyze:import-sort && npm run analyze:prettier && npm run analyze:rustywind",
    "fix:eslint": "eslint --ext .jsx --ext .js . --fix",
    "fix:import-sort": "import-sort --write '**/*.js'",
    "fix:prettier": "prettier --write '**/*.{js,css,md,mdx}'",
    "fix:rustywind": "rustywind --write .",
    "fix": "npm audit fix && npm run fix:import-sort && npm run fix:rustywind && npm run fix:eslint && npm run fix:prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylorbryant/next-starter-tailwind.git"
  },
  "author": "Taylor Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorbryant/next-starter-tailwind/issues"
  },
  "homepage": "https://github.com/taylorbryant/next-starter-tailwind#readme",
  "dependencies": {
    "next": "9.4.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "husky": "4.2.5",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "rustywind": "0.6.5",
    "tailwindcss": "1.4.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write",
    "*.js": [
      "import-sort --write",
      "eslint --fix"
    ]
  }
}
